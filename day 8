#  Create a Car class with attributes and a display method

  class Car:
    def __init__(self, brand, model, year, price):
        self.brand=brand
        self.model=model
        self.year=year
        self.price=price
        
        
    def display(self):
        print(f"Car Details:\nBrand: {self.brand}\nModel: {self.model}\nYear: {self.year}\nPrice: ${self.price}")
        
vehicle = Car("Toyota", "Camry", 2023, 28000)

vehicle.display()
